{
  "name": "dots",
  "version": "2.0.1",
  "main": "js/main.js",
  "scripts": {
    "watch": "mkdir -p build && watchify js/main.js -o build/build.js -dv",
    "build": "mkdir -p build && browserify js/main.js -o build/build.js",
    "serve": "open http://localhost:9875 && python -m SimpleHTTPServer 9875",
    "lint": "eslint ."
  },
  "author": "Taylor Baldwin <taylorbaldwin@gmail.com> (https://tbaldw.in)",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "dat-gui": "^0.5.0",
    "sketch-js": "git+ssh://git@github.com:soulwire/sketch.js.git#e415b8f"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babelify": "^6.4.0",
    "browserify": "^11.2.0",
    "eslint": "^1.7.3",
    "eslint-plugin-react": "^3.6.3",
    "markdownify": "^0.1.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      "markdownify"
    ]
  }
}
